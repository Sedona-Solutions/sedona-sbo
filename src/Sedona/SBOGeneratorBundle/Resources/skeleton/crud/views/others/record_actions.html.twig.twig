<ul class="record_actions">
    <li>
        <a href="{{ "{{ path('admin_" ~ route_name_prefix ~ "_index') }}" }}">
            Back to the list
        </a>
    </li>
{% if ('edit' in actions) and (not hide_edit) %}
    <li>
        <a href="{{ "{{ path('admin_" ~ route_name_prefix ~ "_edit', { 'id': " ~ entity|lower ~ ".id }) }}" }}">
            Edit
        </a>
    </li>
{% endif %}
    {#{% if ('delete' in actions) and (not hide_delete) %}
    <li>{{ '{{ form(delete_form) }}' }}</li>
{% endif %}#}
</ul>
